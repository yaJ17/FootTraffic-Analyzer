[workflow]
name = "Start YouTube Server"
command = "cd flask_backend && python test_youtube.py"
description = "Starts the YouTube streaming server on port 5002"

[workflow]
name = "Start Flask Backend"
command = "cd flask_backend && python run_simple.py"
description = "Starts the Flask backend for video processing on port 5001"

[workflow]
name = "Unified App Startup"
command = "./start_unified.sh"
description = "Starts all services: Node.js app, Flask backend and YouTube streaming"